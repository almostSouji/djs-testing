{
	"name": "boilerplate-next",
	"version": "1.0.0",
	"description": "discord.js boilerplate (next)",
	"main": "index.js",
	"author": "Souji",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "node -r dotenv/config dist/index.js",
		"build": "tsc",
		"dev": "ts-node-dev src/index.ts",
		"lint": "eslint src --ext ts",
		"lint:fix": "eslint src --ext ts --fix"
	},
	"dependencies": {
		"discord.js": "github:discordjs/discord.js",
		"dotenv": "^6.0.0"
	},
	"devDependencies": {
		"@types/dotenv": "^6.1.1",
		"@types/node": "^13.7.1",
		"@types/ws": "^7.2.2",
		"@typescript-eslint/eslint-plugin": "^2.20.0",
		"@typescript-eslint/parser": "^2.20.0",
		"eslint": "^6.8.0",
		"eslint-config-marine": "^6.0.0",
		"ts-node-dev": "^1.0.0-pre.44",
		"typescript": "^3.8.3"
	},
	"eslintConfig": {
		"extends": "marine/node",
		"rules": {
			"no-console": "off"
		}
	}
}
